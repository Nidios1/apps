// iOS Signing Configuration
// This file configures code signing for the iOS app

// Bundle Identifier - MUST match with Apple Developer Portal
PRODUCT_BUNDLE_IDENTIFIER = com.ppapikey.dev

// Code Signing Style - Use automatic for easier management
CODE_SIGN_STYLE = Automatic

// Development Team - Will be set automatically
DEVELOPMENT_TEAM = 

// Code Sign Identity - Will be set automatically
CODE_SIGN_IDENTITY = 

// Provisioning Profile - Will be set automatically
PROVISIONING_PROFILE_SPECIFIER = 

// iOS Deployment Target
IPHONEOS_DEPLOYMENT_TARGET = 13.0

// Disable Bitcode for Flutter compatibility
ENABLE_BITCODE = NO

// Swift Language Version
SWIFT_VERSION = 5.0

// Build Settings for Flutter
FLUTTER_BUILD_MODE = release
FLUTTER_BUILD_NUMBER = 1
FLUTTER_BUILD_NAME = 1.2.2

// Additional settings for proper IPA creation
STRIP_INSTALLED_PRODUCT = YES
COPY_PHASE_STRIP = NO
STRIP_STYLE = all

// Ensure proper framework embedding
ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES

// Code signing settings for installation
CODE_SIGN_INJECT_BASE_ENTITLEMENTS = YES
CODE_SIGN_ENTITLEMENTS = Runner/Runner.entitlements